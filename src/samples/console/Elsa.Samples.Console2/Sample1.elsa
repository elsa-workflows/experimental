// Triggers when HTTP request comes in.
trigger HttpTrigger
{
    Path: "/hello-world",
    SupportedMethods: ["GET", "POST"]
};

// Triggers every 1 minute.
trigger TimerTrigger
{
    Interval: "0:00:01:00"
};

Sequence {
    Activities: [ 
        WriteLine { Text:"writeLine1" },
        Sequence {
            Activities: [
                WriteLine { Text:"writeLine2" }
            ]
        }  
   ]
};